$TTL 86400
@   IN  SOA   {dns_shortname}.{domain_name} root.{domain_name}. (
        2014110701  ;Serial
        7200        ;Refresh
        1800        ;Retry
        604800      ;Expire
        600         ;Minimum TTL
)
@       IN  NS          {dns_shortname}.{domain_name}.
@       IN  A           {dns_fixed_ip}
{dns_shortname}   IN  A           {dns_fixed_ip}
